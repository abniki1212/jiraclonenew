@page
@model jiraclonenew.Pages.Projects.IndexModel
@{
    ViewData["Title"] = "Projects";
}

<h1>Projects</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
@foreach (var project in Model.Projects)
{
        <tr>
            <td>@project.Name</td>
            <td>@project.Description</td>
        </tr>
}
    </tbody>
</table>
